{
  "name": "scientific-quiz",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dist": "npm run copy:resources && npm run copy:scripts && npm run copy:doc && npm pack tmp && npm run copy:package && npm run clean",
    "copy:resources": "node node_modules/cpy-cli/cli.js public/* tmp/public",
    "copy:scripts": "node node_modules/cpy-cli/cli.js src/* tmp/src && node node_modules/cpy-cli/cli.js package.json tmp",
    "copy:doc": "node node_modules/cpy-cli/cli.js swagger*.json tmp",
    "copy:package": "node node_modules/cpy-cli/cli.js synergy-nodejs-integration-*.tgz builds",
    "clean": "node node_modules/del-cli/cli.js './tmp' 'synergy-nodejs-integration-*.tgz'",
    "start": "node src/server.js",
    "start:watch": "nodemon src/server.js",
    "start:dist": "npm install && npm update && pm2 start src/server.js --name synergy-nodejs-integration",
    "stop:dist": "pm2 stop synergy-nodejs-integration",
    "doc": "node node_modules/swagger-jsdoc/bin/swagger-jsdoc -d swaggerDef.json src/**/*.js -o swagger.json",
    "doc:watch": "node node_modules/swagger-jsdoc/bin/swagger-jsdoc -d swaggerDef.json src/**/*.js -o swagger.json -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "history": "^4.7.2",
    "jquery": "^3.2.1",
    "marvinjs": "git+ssh://git.chemaxon.com:9999/marvinjs/marvinjs-dist.git",
    "material-ui": "^1.0.0-beta.19",
    "mongoose": "^4.9.3",
    "node-sass": "^4.5.3",
    "passport": "^0.3.2",
    "passport-local-mongoose": "^4.0.0",
    "passport-openidconnect": "0.0.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "swagger-jsdoc": "^1.9.2",
    "swagger-ui-express": "^1.0.7",
    "webpack-dev-server": "^2.9.4"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "cpy-cli": "^1.0.1",
    "del-cli": "^0.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "webpack": "^3.8.1"
  }
}
