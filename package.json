{
  "name": "synergy-nodejs-integration",
  "version": "1.0.0",
  "description": "ChemAxon Synergy integration with Node.js",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:watch": "nodemon src/server.js",
    "doc": "node node_modules/swagger-jsdoc/bin/swagger-jsdoc -d swaggerDef.json src/**/*.js -o swagger.json",
    "doc:watch": "node node_modules/swagger-jsdoc/bin/swagger-jsdoc -d swaggerDef.json src/**/*.js -o swagger.json -w"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@phabricator.chemaxon.com:9999/source/synergy-nodejs-integration.git"
  },
  "keywords": [
    "chemaxon",
    "synergy",
    "openid",
    "integration",
    "authentication"
  ],
  "author": "Andras Kovacs",
  "contributors": [
    "Attila Biczo"
  ],
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.1",
    "copyfiles": "^1.2.0",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "mongoose": "^4.9.3",
    "passport": "^0.3.2",
    "passport-local-mongoose": "^4.0.0",
    "passport-openidconnect": "0.0.2",
    "swagger-jsdoc": "^1.9.2",
    "swagger-ui-express": "^1.0.7"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
